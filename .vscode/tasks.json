{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"type": "shell",
			"command": "make",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "build_dbg",
			"type": "shell",
			"command": "make config=dbg",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
		,{
			"label": "clean-all",
			"type": "shell",
			"command": "make clean-all",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
		,{
			"label": "test_",
			"type": "shell",
			"command": "make test",
			"problemMatcher": [
				"$gcc"
			],
			"dependsOn": "build",
			"group": "build"
		}
	]
}